import init.smdkc210.common.rc

on fs
    # mount ext4 partitions
    mount ext4 /dev/block/mmcblk0p8 /system ro wait noatime
    mount ext4 /dev/block/mmcblk0p9 /data nosuid nodev noatime wait
    mount ext4 /dev/block/mmcblk0p7 /cache nosuid nodev noatime wait

on boot
    setprop ro.build.product GT-P6810
    setprop ro.product.device GT-P6810

